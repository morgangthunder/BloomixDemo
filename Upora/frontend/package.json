{
    "name":  "upora",
    "version": "0.1.176",
    "scripts":  {
                    "ng":  "ng",
                    "start":  "ng serve --poll 2000 --live-reload true",
                    "start:docker":  "ng serve --host 0.0.0.0 --port 8100 --poll 2000 --live-reload true",
                    "start:fresh":  "ng cache clean \u0026\u0026 ng serve --poll 2000 --live-reload true",
                    "build":  "ng build",
                    "build:dev":  "ng build --configuration development",
                    "watch":  "ng build --watch --configuration development",
                    "test":  "ng test",
                    "cache:clear":  "ng cache clean"
                },
    "prettier":  {
                     "printWidth":  100,
                     "singleQuote":  true,
                     "overrides":  [
                                       {
                                           "files":  "*.html",
